<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="gallery" stopProcessing="true">
                    <match url="^gallery.js" />
                    <action type="Rewrite" url="/js/gallery.php" />
                </rule>
                <rule name="about" stopProcessing="true">
                    <match url="^about.js" />
                    <action type="Rewrite" url="/js/about.php" />
                </rule>
                 <rule name="galleryOdessa" stopProcessing="true">
                    <match url="^galleryOdessa.js" />
                    <action type="Rewrite" url="/js/galleryOdessa.php" />
                </rule>
                <rule name="aboutOdessa" stopProcessing="true">
                    <match url="^aboutOdessa.js" />
                    <action type="Rewrite" url="/js/aboutOdessa.php" />
                </rule>
                <rule name="aboutOdessaDev" stopProcessing="true">
                    <match url="^aboutOdessaDev.js" />
                    <action type="Rewrite" url="/js/aboutOdessaDev.php" />
                </rule>
                <rule name="galleryGindi" stopProcessing="true">
                    <match url="^galleryGindi.js" />
                    <action type="Rewrite" url="/js/galleryGindi.php" />
                </rule>
          </rules>
        </rewrite>
    </system.webServer>
</configuration>